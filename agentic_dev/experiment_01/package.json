{
  "name": "proton-mcp-server",
  "version": "1.0.0",
  "description": "MCP server wrapping Proton CLI for agentic blockchain operations",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "npm run build && node dist/server.js",
    "start:stdio": "npm run build && node dist/stdio-server.js",
    "dev": "ts-node src/server.ts",
    "dev:stdio": "ts-node src/stdio-server.ts",
    "test": "jest",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "mcp",
    "proton",
    "blockchain",
    "agentic"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "ts-node": "^10.9.0",
    "@types/jest": "^29.5.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0"
  }
}
